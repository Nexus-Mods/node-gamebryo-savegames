{
  "name": "gamebryo-savegame",
  "version": "1.2.6",
  "description": "Parses gamebryo savegames",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "prebuild": "node download_lz4.js",
    "build": "autogypi && node-gyp configure build",
    "install": "npm run build && copyasm build dist && copyfiles -f ./lz4/dll/liblz4.dll dist"
  },
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "autogypi": "^0.2.2",
    "copyfiles": "^2.1.0",
    "cross-fetch": "^3.0.1",
    "nbind": "^0.3.15",
    "node-stream-zip": "^1.8.0",
    "node-gyp": "^4.0.0"
  }
}
